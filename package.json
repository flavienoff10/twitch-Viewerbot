{
  "name": "twitch-viewerbot",
  "version": "1.0.0",
  "description": "Projet Node.js avec équivalents des dépendances Python",
  "main": "index.js",
  "scripts": {
    "start": "node index.js"
  },
  "dependencies": {
    "express": "^4.18.2",                    // Flask → Express
    "cors": "^2.8.5",                        // flask-cors → cors
    "jsonwebtoken": "^9.0.0",               // flask-jwt-extended / PyJWT → jsonwebtoken
    "dotenv": "^16.3.1",                     // python-dotenv → dotenv
    "node-fetch": "^3.3.2",                  // requests → node-fetch
    "socks-proxy-agent": "^7.0.0",           // requests[socks] → socks proxy agent
    "streamlink": "npm:streamlink-cli",      // streamlink n'existe pas en npm, alias vers un CLI si installé
    "fake-useragent": "^1.2.0",              // fake-useragent (existe aussi en npm)
    "crypto": "^1.0.1",                      // cryptography → crypto (natif mais parfois wrap)
    "ps-node": "^0.1.6",                     // psutil → ps-node
    "chalk": "^5.3.0"                        // rich → chalk (formatage terminal)
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
